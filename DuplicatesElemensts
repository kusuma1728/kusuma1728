#include <iostream>

using namespace std;



int main()
{
    int arr[]={1, 2, 3, 3, 5, 6, 1, 2};
    int n = sizeof(arr)/sizeof(arr[0]);
    int hash[n] = {0};
    
    for(int i = 0; i < n; i++)
    {
        if(hash[arr[i]] == 1)
            cout << "Duplicate element is present " << arr[i] << endl;
        
        hash[arr]++;
    }
    return 0;
}
